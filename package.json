  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "analyze": "./scripts/analyze.sh",
    "build": "yarn ember build",
    "build:production": "EXPERIMENTAL_RENDER_MODE_SERIALIZE=true yarn build --environment production && sed -i -e 's/{{ROOT_URL}}/\\//g' dist/index.html",
    "build:production:analyze": "CONCAT_STATS=true yarn build:production",
    "bundle-analyze": "yarn broccoli-concat-analyser ./concat-stats-for",
    "clean": "rm -rf dist tmp node_modules concat-stats-for coverage*",
    "debug": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9357 --no-first-run --no-default-browser-check --incognito --user-data-dir=$(mktemp -d -t 'chrome-remote_data_dir')",
    "docs": "ember ember-cli-yuidoc",
    "link:glimmer": "node bin/yarn-link-glimmer.js",
    "lint": "yarn lint:tsc && yarn lint:tslint && yarn lint:eslint && yarn lint:docs",
    "lint:all": "yarn lint:hbs && yarn lint:js && yarn lint:sass",
    "lint:css": "stylelint app/styles",
    "lint:docs": "qunit tests/docs/coverage-test.js",
    "lint:eslint": "eslint . --cache --ext=js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:tslint --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:i18n": "npx ember-intl-analyzer",
    "lint:js": "yarn eslint . --ext .ts --ext .js",
    "lint:sass": "yarn sass-lint --config .sass-lint.yml src/ui/styles/**/*.scss -q -v",
    "lint:tsc": "tsc --noEmit",
    "lint:tslint": "tslint -p tsconfig.json",
    "merge-coverage": "ember coverage-merge",
    "start": "yarn ember serve -p 4201",
    "pretest": "ember build",
    "test": "cross-env SW_DISABLED=true yarn ember exam --random",
    "test:blueprints": "mocha node-tests/blueprints/**/*-test.js",
    "test:browserstack": "node bin/run-browserstack-tests.js",
    "test:ember": "ember test",
    "test:node": "qunit tests/node/**/*-test.js",
    "start:dev": "cross-env SW_DISABLED=true yarn start",
    "test:named": "cross-env SW_DISABLED=true yarn ember exam --partition 1 --split 1 --filter ",
    "test:server": "cross-env SW_DISABLED=true yarn ember exam --server"
  },
  "lint-staged": {
    "*.{js,json,css}": []
  },
  "eslintConfig": {
    "extends": "plugin:unicorn/recommended"
  },
